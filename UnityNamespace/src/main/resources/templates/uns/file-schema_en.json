{
  "name": "Topic",
  "description": "UNS topic",
  "versions": {
    "v1alpha1": {
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "description": "Path ID"
          },
          "alias": {
            "type": "string",
            "description": "Alias"
          },
          "referenceAlias": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Referenced topic alias"
          },
          "frequency": {
            "type": "string",
            "description": "Aggregate topic type: integer + aggregation frequency: hour(h), minute(m), second(s)"
          },
          "namespace": {
            "type": "string",
            "description": "Namespace"
          },
          "expression": {
            "type": "string",
            "description": "Computed topic type - expression"
          },
          "dataType": {
            "type": "integer",
            "description": "1--Time series, 2--Relational, 3--Computed, 6--Aggregated, 7--Referenced"
          },
          "attachment": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "fileType": {
                  "type": "string",
                  "description": "Attachment topic type"
                },
                "fileUrl": {
                  "type": "string",
                  "description": "Attachment topic URL"
                },
                "fileName": {
                  "type": "string",
                  "description": "Attachment topic name"
                }
              }
            }
          },
          "extendProperties": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "key": {
                  "type": "string",
                  "description": "Key information"
                },
                "value": {
                  "type": "string",
                  "description": "Value information"
                }
              }
            }
          },
          "subscribe": {
            "type": "boolean",
            "description": "Subscribed or not"
          },
          "mountSource": {
            "type": "string",
            "description": "Mount source"
          },
          "labels": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "Label ID"
            }
          },
          "displayName": {
            "type": "string",
            "description": "Display name"
          },
          "name": {
            "type": "string",
            "description": "Original name"
          },
          "persistence": {
            "type": "boolean",
            "description": "Persistent or not"
          },
          "definition": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "Field name"
                },
                "type": {
                  "type": "string",
                  "enum": [
                    "INTEGER",
                    "LONG",
                    "FLOAT",
                    "DOUBLE",
                    "BOOLEAN",
                    "DATETIME",
                    "STRING"
                  ],
                  "description": "Field type"
                },
                "maxLen": {
                  "type": "integer",
                  "description": "Maximum length"
                },
                "remark": {
                  "type": "string",
                  "description": "Field description"
                },
                "displayName": {
                  "type": "string",
                  "description": "Display name"
                },
                "systemField": {
                  "type": "boolean",
                  "description": "System field or not"
                }
              }
            }
          },
          "mountType": {
            "type": "integer",
            "description": "0--Non-mounted (created by UNS), 10--Collector, 11--Video collector, 12--Time-series DB connector, 13--GRPC gateway, 14--IoT gateway (MQTT), 15--Simulated collector, 50--Message queue, 60--IT interface, 70--Database"
          },
          "updateDate": {
            "type": "string",
            "description": "Update time"
          },
          "createDate": {
            "type": "string",
            "description": "Creation time"
          },
          "templateId": {
            "type": "integer",
            "description": "Referenced template ID"
          }
        }
      }
    }
  },
  "$metadata": {
    "name": "topic.uns.supos.com",
    "creationTimestamp": "2025-07-09T06:21:57Z",
    "modifiedTimestamp": "2025-07-09T06:31:08Z"
  }
}
